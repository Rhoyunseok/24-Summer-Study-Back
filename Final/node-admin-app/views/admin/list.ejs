

            <!--우측 콘텐츠 영역 -->
            <div class="wrapper wrapper-content">

                <div class="ibox-content m-b-sm border-bottom">
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label class="control-label" for="OrderNo">관리자명</label>
                                <input type="text" class="form-control">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label class="control-label" for="OrderName">아이디</label>
                                <input type="text" class="form-control">
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                <label class="control-label" for="MobileNo">사용여부</label>
                                <input type="text" class="form-control">
                            </div>
                        </div>
                    </div>
                    <div class="text-center">
                        <button type="button" class="btn btn-primary">조회</button>
                        <a href="/admin/create" class="btn btn-info">관리자 계정 등록</a>
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-content">
                                <div class="table-responsive">

                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th width="10%">아이디</th>
                                                <th width="10%">성명</th>
                                                <th width="20%">메일주소</th>
                                                <th width="20%">소속회사</th>
                                                <th width="10%">부서</th>
                                                <th width="10%">사용여부</th>
                                                <th width="20%">등록일</th>
                                            </tr>
                                        </thead>
                                        <tbody class="hoverTblBody" id = "list">

                                            <% for(var i=0;i<admins.length;i++){ %>
                                                <tr>
                                                    <td><a href="/admin/modify/<%=admins[i].admin_no%>"><%=admins[i].admin_id%></a></td>
                                                    <td><%=admins[i].admin_name%></a></td>
                                                    <td><%=admins[i].admin_email%></td>
                                                    <td><%=admins[i].admin_company%></td>
                                                    <td><%=admins[i].admin_cp%></td>
                                                    <td>
                                                        <% if(admins[i].admin_use == 1) { %>
                                                            사용함
                                                        <% } else if (admins[i].admin_use == 2){ %>
                                                            사용안함
                                                        <% } %>
                                                    </td>
                                                    <td><%=admins[i].admin_regist_date%></td>
                                                </tr>
                                            <% } %>
                                            <!-- <tr>
                                                <td><a href="#">code.AdminID</a></td> 
                                                <td>code.AdminName</td>
                                                <td>code.AdminEmail</td> 
                                                <td>code.Company</td> 
                                                <td>code.CompanyPosition</td>
                                                <td>code.UseYN</td> 
                                                <td>Date</td> 
                                            </tr> -->


                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>
                    </div>

                </div>


            </div>

            <!--우측 하단 풋터 영역-->
            <div class="footer">
                <div class="pull-right">
                    10GB of <strong>250GB</strong> Free.
                </div>
                <div>
                    <strong>Copyright</strong> Example Company &copy; 2014-2018
                </div>
            </div>
